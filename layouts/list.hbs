<!doctype html>
<html lang="sv">

<head>
  <meta charset="UTF-8">
  {{#if pagination.index}}
  <title>Sida {{ pagination.num }} | {{ name }}</title>
  {{else}}
  <title>{{ name }}</title>
  {{/if}}
  <meta name="viewport" content="width=device-width">
  <link rel="stylesheet" title="madr.se stilmall" href="../madrse.css">
  <link href="../prenumerera.xml" type="application/atom+xml" rel="alternate" title="madr.se">
  {{#unless pagination.index}}
  <meta name="robots" content="noindex,follow">
  {{/unless}}
  {{#if pagination.previous}}
  <link rel="previous" href="/{{ pagination.previous.path }}">
  {{/if}}
  {{#if pagination.next}}
  <link rel="next" href="/{{ pagination.next.path }}">
  {{/if}}
</head>

<body>
  {{> header }}
  <main id="content">
    <h1>Sida {{ pagination.num }}</h1>
    {{#each pagination.files}}
    {{#if location}}
    {{> teaser-event}}
    {{else}}
    {{#if album}}
    {{> teaser-brutal}}
    {{else}}
    {{#if link}}
    {{> teaser-link}}
    {{else}}
    {{> teaser-post }}
    {{/if}}
    {{/if}}
    {{/if}}
    {{/each}}
    <div class="pagination">
      {{#if pagination.previous}}
      <a rel="previous" href="../{{ pagination.previous.path }}">← Yngre
        inlägg</a>
      {{/if}}
      {{#if pagination.next}}
      <a rel="next" href="../{{ pagination.next.path }}">Äldre inlägg →</a>
      {{/if}}
    </div>
  </main>
  {{> aside }}
  {{> footer }}
</body>

</html>